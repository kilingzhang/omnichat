{
  "name": "@omnichat/example",
  "version": "0.1.0",
  "description": "Example usage of Universal IM SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bots/group-assistant/index.js",
    "dev": "tsx src/bots/group-assistant/index.ts"
  },
  "dependencies": {
    "@omnichat/core": "workspace:*",
    "@omnichat/telegram": "workspace:*",
    "@omnichat/discord": "workspace:*"
  },
  "peerDependencies": {
    "node-telegram-bot-api": "^0.67.0",
    "discord.js": "^14.14.0"
  },
  "peerDependenciesMeta": {
    "node-telegram-bot-api": {
      "optional": true
    },
    "discord.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
